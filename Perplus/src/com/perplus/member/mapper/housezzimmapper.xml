<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="houseZzim">
	<insert id="insertHouseZzim" parameterType="HouseZzimVo">
		insert into HOUSEZZIM values(#{houseSerial},#{memberEmail})
	</insert>
	
	<delete id="deleteHouseZzim" parameterType="hashmap">
		delete from HOUSEZZIM
		where	house_serial = #{houseSerial}
		and		member_email = #{memberEmail}
	</delete>
	
</mapper>